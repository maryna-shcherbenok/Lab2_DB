@model IEnumerable<object>
@{
    ViewData["Title"] = "Результати запиту 5";
}

<h2>Результати запиту 5</h2>

@if (!Model.Any())
{
    <p>Немає даних для відображення.</p>
}
else
{
    <table class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Бібліотека</th>
                <th>Керівник</th>
                <th>Телефон</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.GetType().GetProperty("LibraryName")?.GetValue(item)</td>
                    <td>@item.GetType().GetProperty("LeaderName")?.GetValue(item)</td>
                    <td>@item.GetType().GetProperty("LeaderPhone")?.GetValue(item)</td>
                    <td>@item.GetType().GetProperty("LeaderEmail")?.GetValue(item)</td>
                </tr>
            }
        </tbody>
    </table>
}

<a class="btn btn-primary mt-3" asp-action="Index" asp-controller="Queries">Назад</a>
